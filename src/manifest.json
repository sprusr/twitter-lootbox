{
  "name": "Twitter Lootbox",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "34": "icon-34.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.bundle.js"]
    }
  ]
}
